<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.administrator.ForkliftHMI.Logs">

    <!-- TODO: Update blank fragment layout -->

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <ProgressBar
            android:id="@+id/line_progressBar_pallet"
            style="@style/CustomProgressBarHorizontal"
            android:layout_width="333dp"
            android:layout_height="37dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:progress="30"
            app:layout_constraintStart_toEndOf="@+id/textView4"
            app:layout_constraintTop_toBottomOf="@+id/line_progressBar_pallet2" />

        <ProgressBar
            android:id="@+id/line_progressBar_pallet5"
            style="@style/CustomProgressBarHorizontal"
            android:layout_width="333dp"
            android:layout_height="37dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:progress="26"
            app:layout_constraintStart_toEndOf="@+id/textView5"
            app:layout_constraintTop_toBottomOf="@+id/line_progressBar_pallet" />

        <ProgressBar
            android:id="@+id/line_progressBar_pallet4"
            style="@style/CustomProgressBarHorizontal"
            android:layout_width="333dp"
            android:layout_height="37dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:progress="22"
            app:layout_constraintStart_toEndOf="@+id/textView8"
            app:layout_constraintTop_toBottomOf="@+id/line_progressBar_pallet5" />

        <ProgressBar
            android:id="@+id/line_progressBar_pallet3"
            style="@style/CustomProgressBarHorizontal"
            android:layout_width="333dp"
            android:layout_height="37dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:progress="12"
            app:layout_constraintStart_toEndOf="@+id/textView7"
            app:layout_constraintTop_toBottomOf="@+id/line_progressBar_pallet4" />

        <ProgressBar
            android:id="@+id/line_progressBar_pallet2"
            style="@style/CustomProgressBarHorizontal"
            android:layout_width="333dp"
            android:layout_height="37dp"
            android:layout_marginStart="8dp"
            android:progress="32"
            app:layout_constraintStart_toEndOf="@+id/textView3"
            app:layout_constraintTop_toTopOf="@+id/textView3" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="450dp"
            android:layout_height="37dp"
            android:layout_marginTop="8dp"
            android:text="Este mes:           1154↑      877↓"
            android:textSize="22sp"
            app:layout_constraintStart_toStartOf="@+id/textView8"
            app:layout_constraintTop_toBottomOf="@+id/textView8" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="450dp"
            android:layout_height="37dp"
            android:layout_marginTop="8dp"
            android:text="Esta semana:      235↑      198↓          "
            android:textSize="22sp"
            app:layout_constraintStart_toStartOf="@+id/textView5"
            app:layout_constraintTop_toBottomOf="@+id/textView5" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="450dp"
            android:layout_height="37dp"
            android:layout_marginTop="8dp"
            android:text="Última día:             61↑        48↓"
            android:textSize="22sp"
            app:layout_constraintStart_toStartOf="@+id/textView4"
            app:layout_constraintTop_toBottomOf="@+id/textView4" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="450dp"
            android:layout_height="37dp"
            android:layout_marginTop="8dp"
            android:text="Último turno:        32↑        44↓"
            android:textSize="22sp"
            app:layout_constraintStart_toStartOf="@+id/textView3"
            app:layout_constraintTop_toBottomOf="@+id/textView3" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="450dp"
            android:layout_height="37dp"
            android:layout_marginTop="104dp"
            android:text="Última hora:             8↑       12↓"
            android:textSize="22sp"
            app:layout_constraintStart_toStartOf="@+id/line_progressBar_pallet1"
            app:layout_constraintTop_toBottomOf="@+id/line_progressBar_pallet1" />

        <ProgressBar
            android:id="@+id/line_progressBar_pallet1"
            style="@style/CustomProgressBarHorizontal"
            android:layout_width="790dp"
            android:layout_height="68dp"
            android:layout_marginEnd="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="208dp"
            android:progress="32"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="339dp"
            android:layout_height="49dp"
            android:text="Uso del almacén:"
            android:textSize="32sp"
            app:layout_constraintBottom_toTopOf="@+id/line_progressBar_pallet1"
            app:layout_constraintStart_toStartOf="@+id/line_progressBar_pallet1" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="sans-serif"
            android:text="32%"
            android:textSize="128dp"
            app:layout_constraintBottom_toTopOf="@+id/line_progressBar_pallet1"
            app:layout_constraintEnd_toEndOf="@+id/line_progressBar_pallet1" />

    </android.support.constraint.ConstraintLayout>

</FrameLayout>
